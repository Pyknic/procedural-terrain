#	for j in range(0, floor(bitsSet / 3)):
#		# Compute the normal of the triangle as the cross product between two edges
#		var crossA : Vector3 = vertices[vertices.size() - bitsSet + j * 3]
#		var crossB : Vector3 = vertices[vertices.size() - bitsSet + j * 3 + 1]
