#		var normal : Vector3 = (crossC - crossB).cross(crossC - crossA).normalized()
#		for j in range(0, 3):
#			normals.push_back(normal)
	
	# Each row in the TRIANGLE_TABLE can consist of up to 5 triangles
	for i in range(0, 5):
		if TRIANGLE_TABLE[cubeindex][i * 3] >= 0:
			# Iterate over the corners of the triangle
			for j in range(0, 3):
				var edgeIdx : int = TRIANGLE_TABLE[cubeindex][i * 3 + j]
				var cornerA : Vector3 = CUBE_CORNERS[CORNER_INDEX_A_FROM_EDGE[edgeIdx]]
				var cornerB : Vector3 = CUBE_CORNERS[CORNER_INDEX_B_FROM_EDGE[edgeIdx]]
				#var cornerMean = (cornerA + cornerB) / 2

				# Compute the braking point of the surface along the edge
				var densityA = density(pos + size * cornerA)
				var densityB = density(pos + size * cornerB)
				var interpFactor = (surface - densityA) / (densityB - densityA)
				var cornerIntrp = cornerA + (cornerB - cornerA) * interpFactor
				var vertice = pos + size * cornerIntrp

				# If everything is working, then all visible triangles 
				# should have roughly the same color
				var gray = (density(vertice) - densityMin) / densityRange
				colors.push_back(Color(gray, gray, gray))

				indices.push_back(vertices.size())
				vertices.push_back(vertice)
				uvs.push_back(Vector2(vertice.x, vertice.z))

			# Compute the normal of the triangle as the cross product between two edges
			var crossA : Vector3 = vertices[vertices.size() - 3]
			var crossB : Vector3 = vertices[vertices.size() - 2]
			var crossC : Vector3 = vertices[vertices.size() - 1]
			var normal : Vector3 = (crossC - crossB).cross(crossC - crossA).normalized()
			for j in range(0, 3):
				normals.push_back(normal)

	pass
	
