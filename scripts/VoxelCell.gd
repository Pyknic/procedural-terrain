func addVoxel(pos : Vector3, size : float, densityMin : float, densityRange : float):
	
	# Build a bitset of the status of the eight corners in the cube
	var cubeindex = 0;
	for i in range(0, 8):
		var p = pos + size * CUBE_CORNERS[i]
		if density(p) < surface:
			cubeindex |= (0x1 << i)
			
