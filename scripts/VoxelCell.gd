tool
extends MeshInstance

var noise = OpenSimplexNoise.new()

export var size        = 10 setget setSize
export var octaves     = 4 setget setOctaves
export var period      = 20.0 setget setPeriod
export var persistence = 0.8 setget setPersistence
export var surface     = 0.0 setget setSurface
export var noiseSeed   = 0.0 setget setNoiseSeed
export var cubeSize    = 0.5

var vertices = PoolVector3Array()
var normals  = PoolVector3Array()
var colors   = PoolColorArray()
var indices  = PoolIntArray()
var uvs      = PoolVector2Array()

# Called when the node enters the scene tree for the first time.
func _ready():
	noise.seed = noiseSeed
	noise.octaves = octaves
	noise.period = period
	noise.persistence = persistence
	updateVoxel()
	pass

func setSize(s):
	size = s
	updateVoxel()
	pass

func setOctaves(val):
	octaves = val
	noise.octaves = val
	updateVoxel()
	pass
	
func setPeriod(val):
	period = val
	noise.period = val
	updateVoxel()
	pass
	
func setPersistence(val):
	persistence = val
	noise.persistence = val
	updateVoxel()
	pass
	
func setSurface(val):
	surface = val
	updateVoxel()
	pass
		
func setNoiseSeed(val):
	noiseSeed = val
	noise.seed = val
	updateVoxel()
	pass

func density(at : Vector3):
	return noise.get_noise_3dv(translation + at) - at.y / 10

func updateVoxel():
	vertices.resize(0)
	normals.resize(0)
	colors.resize(0)
	indices.resize(0)
	uvs.resize(0)
	
	var arrayMesh = ArrayMesh.new()

	var minDensity = density(Vector3(0, 0, 0))
	var maxDensity = minDensity
	for i in range(-size, size):
		for j in range(-size, size):
			for k in range(-size, size):
				var pos = Vector3(i, j, k)
				var d = density(pos)
				if d < minDensity:
					minDensity = d
				if d > maxDensity:
					maxDensity = d
	
	var densityRange = maxDensity - minDensity
	if densityRange > 0:
		for i in range(-size, size):
			for j in range(-size, size):
				for k in range(-size, size):
					var pos = Vector3(i, j, k)
					addVoxel(pos, cubeSize, minDensity, densityRange)

		smoothNormals()

		var arrays = []
		arrays.resize(ArrayMesh.ARRAY_MAX)
		arrays[ArrayMesh.ARRAY_INDEX]  = indices
		arrays[ArrayMesh.ARRAY_VERTEX] = vertices
		arrays[ArrayMesh.ARRAY_NORMAL] = normals
		arrays[ArrayMesh.ARRAY_COLOR]  = colors
		arrays[ArrayMesh.ARRAY_TEX_UV] = uvs
		arrayMesh.add_surface_from_arrays(Mesh.PRIMITIVE_TRIANGLES, arrays)

		self.mesh = arrayMesh
	pass
	
func smoothNormals():
	var map = Dictionary()
	for i in range(0, vertices.size()):
		var vertice  = vertices[i]
		var normal   = normals[i]
		if map.has(vertice):
			map[vertice].push_back([i, normal])
		else:
			map[vertice] = [[i, normal]]
			
	for normalList in map.values():
		var mean = normalList[0][1]
		for i in range(1, normalList.size()):
			mean = mean + normalList[i][1]
		mean = mean / normalList.size()
		for i in range(0, normalList.size()):
			normals[normalList[i][0]] = mean
	pass

